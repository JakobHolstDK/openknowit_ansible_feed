#!/usr/bin/env bash
source ~/venv/bin/activate
#source /etc/vaultenvironment

epoch=$(date +"%s")
vault kv put secret/aaoaa epoch=$epoch >/dev/null 2>&1
vault kv get secret/aaoaa  2>/dev/null |jq .data.data.epoch -r |grep $epoch
if [[ $? == 0 ]];
then
    echo "`date`: We have an active vault"
else
    echo "`date`: We do not have an active vault"
    tmux "bin/vault server -dev" -A -s myprogramsession \; send -t myprogramsession "nohup bin/vault server -dev &>/dev/null &" ENTER \; 
fi

#/opt/openknowit_ansible_feed/bin/awx_manager.py

	

