#!/usr/bin/env bash
source ~/venv/bin/activate
source /etc/vaultenvironment

STATUS="RUNNING"

while [[ ${STATUS} != "TERMINATE" ]];
do
	if [[ -f /opt/openknowit_ansible_feed/etc/master.json ]];
	then
		/opt/openknowit_ansible_feed/bin/awx_manager.py master
	fi
	if [[ -f /opt/openknowit_ansibleautomation_main/etc/custom.json ]];
	then
		/opt/openknowit_ansible_feed/bin/awx_manager.py custom
	fi
	sleep 60
done
	

